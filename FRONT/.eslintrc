{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:vue/recommended"
  ],
  "plugins": [],
  "rules": {
    "vue/mushtache-interpolation-spacing": "off",
    "vue/max-attributes-per-line": "off",
    "vue/singleline-element-content-newline": "off"
  }
}